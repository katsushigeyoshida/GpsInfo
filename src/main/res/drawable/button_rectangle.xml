<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 背景色 -->
    <solid android:color="#FF6200EE" />

    <!-- 角丸の設定（四角形の場合は0dp） -->
    <corners android:radius="0dp" />

    <!-- 枠線の設定（任意） -->
    <stroke
        android:width="2dp"
        android:color="#000000" />
</shape>